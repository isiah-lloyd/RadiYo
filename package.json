  "description": "A Discord Bot to play internet radio streams",
  "main": "index.js",
  "scripts": {
    "start": "npm run tsc && node dist/index.js",
    "dev": "npm run tsc && NODE_ENV=development node --inspect --enable-source-maps dist/index.js",
